<div class="signup-background" class="form-control">
      
    <div id=signupForm>
        <div>
            <label for="first_name">First Name</label>
            <input type="text" class="signforElements, form-control" id="first_name"></input>
        </div>
        <div>
            <label for="last_name">Last Name</label>
            <input type="last_name" class="signforElements, form-control" id="last_name"></input>
        </div>
        <div>
            <label for="email">Email address</label>
            <input type="email" class="signforElements, form-control" id="email" aria-describedby="emailHelp">
            {{!-- <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> --}}
        </div>
        <div>
            <label for="username">Screen Name</label>
            <input type="text" class="signforElements, form-control" id="username"></input>
        </div>
        <div>
            <label class=passBtn for="password">Password</label>
            <input type="password" class="signforElements, form-control" id="password"></input>
        </div>
        <div>
            <button type="button" class="signforButton btn btn-primary" id="signupButton">SignMeUpPLZDADDY</button>
        </div>
        <div>
            <br>
            <br>
        </div>
        <div>
            <button data-toggle="modal" data-target="#signinModal" type="button" class="signinButton btn btn-primary" id="signinBtn">SIGN IN</button>
        </div>
    </div>
     

    
</div>